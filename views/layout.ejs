      <!-- CSS -->
      <style>
        /** Global **/
        *{
          margin: 0;
          padding: 0;
          list-style: none;
          text-decoration: none;
          box-sizing: border-box;
          font-family: 'Montserrat', sans-serif;
    
        }

        body {
          background-color: #f8f8f8 !important;
        }

        #env-selecter {
          background-color: #EEF1FC  !important;
          box-sizing: border-box;
                box-shadow: 0 15px 25px rgba(0,0,0,.6);
                border-radius: 10px;
               padding: 10px;
        }

        .selecter {
          margin-bottom: 10px;
          position: relative;
        }

        .nav-a small {
          color: #222;

        }

        .nav-a small:hover{
          text-decoration: underline;
        }
    
        nav {
          background-color: #222 !important;
        }
    
        .search {
          margin-left: 100px;
          padding-left: 100px;
        }

        #index_content {
          margin-right: 40px;
          
        }

        #user_manual {
          position: fixed;
          bottom: 100px;
          right: 0px;
        }

        #table-index {
          margin-top: 50px;
          margin-left: 50px;
        }

        .cards .card {
          background: #fff;
          border-radius: 5px;
          padding: 20px;
          width: 300px;
          display: inline-flex;
          margin: 5px;
          
        }

        .cards .cards .result{
          padding-top: 5px;
        }

        #add-form-box #last-button{
          margin-bottom: 15px;
        }
    
        #navigation li a {
          text-decoration:none;
        }
        
        #navigation li a.on {
          background:yellow;
        }

        #mobile-menu {
          display: none;
                }

        #mobile-menu-button {
          display: none;

        }
        
        ul li a.active {
          text-decoration: none;
          border-bottom: #4285E9 0.125em solid
        }

        .footer {
            position: fixed;
            bottom: 0;
            width: 120%;
            background-color: #222;
            color: white;
            
            height:60px;
            
        }
        .footer p {
          color: #fff;
        }

        /** Responsive  */
        @media only screen and (min-width: 768px) {
        .add-form-box {
          padding-left: 100px;
          padding-right: 100px;
        }
      }

      /* Extra small devices (phones, 600px and down) */
@media only screen and (max-width: 600px) {

  body {
    height: 130%;
  }
  #mobile-menu {
    display: inline;
    margin-top: 25px;
  }

        #mobile-menu-button {
          display: inline;

        }

  #navigation {
    display: none;
  }

  #profile-button {
    display: none;
  }

}


      </style>

