
<!-- Search style -->
 <style>
   .table-scroll {
     overflow:auto;  
    }

    #search-bar {
      margin-bottom: 20px;
    }
    
    #search-bar input {
      width: 25%;
      height: 25px;
      border-radius: 3px;
    }
          
</style>
        
 <div  id="search-bar">
          <form action="/home/search" method="GET">
            <input  type="search" name="search_value" placeholder=" 🔍 by name " >
            <button type="submit"> </button>
          </form>
</div>
   
<div class="uk-overflow-auto">
  <div class="table-scroll">
        <table class="uk-table uk-table-striped uk-table-small" id="Table-data">
          <thead>
            <tr>
              <th scope="col"></th>
              <th scope="col">Full Name </th>
              <th scope="col"> Anxiety type </th>
              <th scope="col"> Anxiety Status </th>
              <th scope="col"> Edit </th>
              <th scope="col"> Unlock Enviroment </th>
              <th scope="col"> Delete </th>
            </tr>
          </thead>

          <tbody>
            <%if (typeof data != 'undefined' && data.length > 0) { %>
              <% var i = 1 %> 
              <% var string = '' %> 
              <% data.forEach(function(row){ %>
            <tr>
              <td> <%= i++%>  </td>
              <td> <%= string.concat(row.first_name + "  " + row.last_name)  %> </td>
              <td> <%= row.anxiety_type %> </td>
              <td>
                <a href="/list/chart/:<%=row.player_id%> ">
                  <span class="title">
                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-bar-chart-line" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                      <path fill-rule="evenodd" d="M11 2a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v12h.5a.5.5 0 0 1 0 1H.5a.5.5 0 0 1 0-1H1v-3a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v3h1V7a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v7h1V2zm1 12h2V2h-2v12zm-3 0V7H7v7h2zm-5 0v-3H2v3h2z"/>
                    </svg>
                    view
                  </span>
                </a>
              </td>

              <td>
                <a href="/list/:<%=row.player_id%> /edit">
                  <span class="title">
                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-pencil-square" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                      <path d="M15.502 1.94a.5.5 0 0 1 0 .706L14.459 3.69l-2-2L13.502.646a.5.5 0 0 1 .707 0l1.293 1.293zm-1.75 2.456l-2-2L4.939 9.21a.5.5 0 0 0-.121.196l-.805 2.414a.25.25 0 0 0 .316.316l2.414-.805a.5.5 0 0 0 .196-.12l6.813-6.814z"/>
                      <path fill-rule="evenodd" d="M1 13.5A1.5 1.5 0 0 0 2.5 15h11a1.5 1.5 0 0 0 1.5-1.5v-6a.5.5 0 0 0-1 0v6a.5.5 0 0 1-.5.5h-11a.5.5 0 0 1-.5-.5v-11a.5.5 0 0 1 .5-.5H9a.5.5 0 0 0 0-1H2.5A1.5 1.5 0 0 0 1 2.5v11z"/>
                    </svg>
                    edit
                  </span>
                </a>
              </td>

              <td>
                <a href="/AdminRequest/addEnv/:<%=row.player_id%> ">
                  <span class="title">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-unlock" viewBox="0 0 16 16">
                      <path fill-rule="evenodd" d="M9.655 8H2.333c-.264 0-.398.068-.471.121a.73.73 0 0 0-.224.296 1.626 1.626 0 0 0-.138.59V14c0 .342.076.531.14.635.064.106.151.18.256.237a1.122 1.122 0 0 0 .436.127l.013.001h7.322c.264 0 .398-.068.471-.121a.73.73 0 0 0 .224-.296 1.627 1.627 0 0 0 .138-.59V9c0-.342-.076-.531-.14-.635a.658.658 0 0 0-.255-.237A1.122 1.122 0 0 0 9.655 8zm.012-1H2.333C.5 7 .5 9 .5 9v5c0 2 1.833 2 1.833 2h7.334c1.833 0 1.833-2 1.833-2V9c0-2-1.833-2-1.833-2zM8.5 4a3.5 3.5 0 1 1 7 0v3h-1V4a2.5 2.5 0 0 0-5 0v3h-1V4z"/>
                    </svg>
                    unlock enviroment
                  </span>
                </a>
              </td>

              <td>
                <a class="uk-button uk-button-danger uk-margin uk-button-small"
                href="/list/delete/:<%=row.player_id%>"
                > 
               Delete </a>
              </td>

            </tr>
            <% })} else { %>
              <% if (typeof found != 'undefined') {%> 
                <% if(found == 'no') {%> 
                  <tr>
                    <td colspan="6"> No patient found </td>
                    
                  </tr>
                <% } else { %> 
                <tr>
                  <td colspan="6">No records is there yet</td> 
                </tr>
              <% } } }%>
          </tbody>
        </table>
  </div>
</div>