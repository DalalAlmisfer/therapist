
            <!-- List of Children  --> 
            <table >
                <thead>
                    <tr>
                        <th> id </th>
                        <th scope="col">  Full Name </th> 
                        <th scope="col">  Title </th>
                        <th scope="col">  Anxiety type </th>
                        <th scope="col">  Status        </th>
                        <th scope="col">  Accept     </th>
                        <th scope="col">  Terminate     </th>


                     </tr>
                </thead>
        
                <tbody id="Table-data">
                    <% if(typeof data != 'undefined' ){ %>
                        <% var string = '' %> 
                        <% var i = 1 %> 
                        <%  data.forEach(function(row)  { %>
                    <tr>
                        <td> <%= i++ %> </td>
                        <td> <%= string.concat(row.first_name,"  " ,row.last_name) %> </td>
                        <td> <%= row.env_title %> </td>
                        <td> <%= row.anxiety_type %> </td>
                           <% if(row.accepted_env == 2){ %> 
                            <td> accepted </td>
                           <% } else if (row.accepted_env == 1){ %> 
                                 <td> rejected </td>
                                 <% } else {%> 
                                <td> Not decieded yet </td>
                                <% } %> 

                        <td>
                            <a  href="/AddRequest/:<%=row.player_id %>/decision/accept" > accept </a>
                        </td> 
                        <td> 
                            <a href="/AddRequest/:<%=row.player_id %>/decision/terminate" > terminate </a>
                        </td>
                    </tr>

                    <% })} else{ %>
                        <tr>
                          <td colspan="6">No Record Found</td>
                          
                        </tr>
                      <% } %>
                      
                </tbody>
            </table>

         